    agent any
        stages {
        stage('Initialize'){
            steps{
                echo "Esta es el inicio"
            }
        }
        stage('Build') {
            steps {
                sh 'java --version'
            
            }
        }
            
        stage('Test') {
            steps {
                 sh "echo" 
            
            }
        } 
        
            } 
}
